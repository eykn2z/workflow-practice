{
  "containerDefinitions": [
    {
      "cpu": 0,
      "essential": true,
      "image": "httpd",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/hello-world",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "name": "hello-world",
      "portMappings": [
        {
          "appProtocol": "791848147212.dkr.ecr.us-east-1.amazonaws.com/api-server:latest",
          "containerPort": 80,
          "hostPort": 80,
          "name": "hello-world-80-tcp",
          "protocol": "http"
        }
      ]
    }
  ],
  "cpu": "256",
  "executionRoleArn": "arn:aws:iam::791848147212:role/ecsTaskExecutionRole",
  "family": "hello-world",
  "ipcMode": "",
  "memory": "512",
  "networkMode": "awsvpc",
  "pidMode": "",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "tags": [
    {
      "key": "env",
      "value": "default"
    },
    {
      "key": "created_by",
      "value": "terraform"
    }
  ]
}
